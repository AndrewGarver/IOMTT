// list of unique places to be built as map targets
var uniquePlaces = [];
for (i = 0; i < data.length; i++) { 
  uniquePlaces.push( data[i].Place ) 
};
uniquePlaces = uniquePlaces.filter(function(item, pos) {
    return uniquePlaces.indexOf(item) == pos;
  }
);
uniquePlaces.sort();

// for getting formatted name from circle ID
var uniquePlacesHash = {};
for (i=0; i<uniquePlaces.length; i++) {
  var place = uniquePlaces[i].replace(/ /g,"").replace(/'/g,"").toLowerCase();
  uniquePlacesHash[place] = uniquePlaces[i];
}

// for comparring rider name as circle id with original
var uniqueRiderHash = {};
for (i = 0; i<data.length; i++) {
  var idName = (data[i].Rider).replace(/ /g,"").replace(/\./g,"").replace(/'/g,"").replace(/’/g,"").replace(/-/g,"").replace(/\(/g,"").replace(/\)/g,"").toLowerCase();
  uniqueRiderHash[idName] = data[i].Rider;
}
